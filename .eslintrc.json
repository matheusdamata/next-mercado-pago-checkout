{
  "extends": ["next/core-web-vitals", "next/typescript", "@rocketseat/eslint-config/next"],
  "plugins": [
    "unused-imports"
  ],
  "rules": {
    "no-use-before-define": "off",
    "camelcase": "off",
    "no-unused-vars": "off",
    "unused-imports/no-unused-imports": "warn",
    "unused-imports/no-unused-vars": [
      "warn",
      {
        "vars": "all",
        "varsIgnorePattern": "^_",
        "args": "after-used",
        "argsIgnorePattern": "^_"
      }
    ]
  }
}
